{
  "name": "dature",
  "version": "0.3.5",
  "description": "新浪博客抓取程序",
  "main": "cli.js",
  "bin": {
    "dature": "cli.js"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "is-url": "^1.2.2",
    "mkdirp": "^0.5.1",
    "yargs": "^11.0.0",
    "iconv-lite": "^0.4.21"
  },
  "devDependencies": {
    "pkg": "^5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junyiz/dature.git"
  },
  "bugs": {
    "url": "https://github.com/junyiz/dature/issues"
